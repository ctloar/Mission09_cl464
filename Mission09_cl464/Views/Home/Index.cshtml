@{
    ViewData["Title"] = "Home Page";
}
@model BooksViewModel

<link href="~/lib/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet" />

<h1>Bookstore</h1>

@foreach (Book b in Model.Books)
{
    <partial name="BookstoreSummary" model="b" />
}

<div page-tag="@Model.PageInfo" page-action="Index" page-classes-enabled="true"
     page-class="btn" page-class-normal="btn-outline-dark"
     page-class-selected="btn-primary" class="btn-group pull-right m-1 center"></div>
